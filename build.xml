<project name="BookTrading" default="compile" basedir=".">
	<property name="src" location="src" />
	<property name="lib" location="lib" />
	<property name="build" location="classes" />
	<property name="dist" location="dist" />
	<property name="classpath" value="${lib}/jade.jar;${lib}/jadeTools.jar;${lib}/http.jar;${lib}/iiop.jar;${lib}/commons-codec-1.3.jar" />

	<target name="compile">
		<javac srcdir="${src}" destdir="${build}" classpath="${classpath}" includeantruntime="no" />
	</target>

</project>